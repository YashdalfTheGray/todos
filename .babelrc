{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "useBuiltIns": "usage",
      "corejs": {
        "version": "3.6",
        "proposals": true
      }
    }],
    "@babel/preset-react"
  ],
  "plugins": ["react-hot-loader/babel", "@babel/plugin-proposal-class-properties"]
}
