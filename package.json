  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/YashdalfTheGray/todos/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "homepage": "https://github.com/YashdalfTheGray/todos#readme",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
