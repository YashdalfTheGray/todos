      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "homepage": "https://github.com/YashdalfTheGray/todos#readme",
  "dependencies": {
    "@material-ui/core": "^4.2.1",
